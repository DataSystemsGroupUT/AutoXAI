        "# 6) Batch run over DATASETS + meta-recommendation via nearest neighbor\n",
        "import gc\n",
        "all_results = {}\n",
        "enc_stats = {}\n",
        "bb_scores = {}\n",
        "\n",
        "\n",
        "for name in DATASETS:\n",
        "    print(\"Processing dataset: \",name)\n",
        "    X, y = load_dataset(name)\n",
        "    if X is None: continue\n",
        "    try:\n",
        "        X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.3, random_state=RANDOM_STATE, stratify=y if len(pd.unique(y))>1 else None)\n",
        "        pipe = make_pipeline(X, use_autosklearn=USE_AUTOSKLEARN, rs=RANDOM_STATE)\n",
        "        t0=time.time(); pipe.fit(X_train, y_train); train_time=time.time()-t0\n",
        "        acc = accuracy_score(y_test, pipe.predict(X_test))\n",
        "        print('[RUN] %-20s acc=%.3f train_time=%.1fs' % (name, acc, train_time))\n",
        "        bb_scores[name]=acc\n",
        "        expl = run_explainers(pipe, X_train, X_test, max_lime=MAX_LIME_SAMPLES, max_anchor=MAX_ANCHOR_SAMPLES, rf_depth=RULEFIT_MAX_DEPTH, rs=RANDOM_STATE)\n",
        "        rows=[]\n",
        "        lw,ll,tl = expl['LIME']; rows.append(dict(method='LIME', entropy_ratio=entropy_ratio(lw), kl=kl_to_uniform(lw), gini=gini_coeff(lw), compactness=compactness_from_weights(lw), speed=tl, identity=identity_score(ll), separability=separability_score(ll)))\n",
        "        aw,al,ta = expl['ANCHOR']; rows.append(dict(method='ANCHOR', entropy_ratio=entropy_ratio(aw), kl=kl_to_uniform(aw), gini=gini_coeff(aw), compactness=compactness_from_weights(aw), speed=ta, identity=identity_score(al), separability=separability_score(al)))\n",
        "        rw,rl,fid = expl['RuleFit']; rows.append(dict(method='RuleFit', entropy_ratio=entropy_ratio(rw.reindex(X.columns, fill_value=0)), kl=kl_to_uniform(rw.reindex(X.columns, fill_value=0)), gini=gini_coeff(rw.reindex(X.columns, fill_value=0)), compactness=compactness_from_weights(rw.reindex(X.columns, fill_value=0)), speed=np.nan, identity=identity_score(rl), separability=separability_score(rl), correctness=fid))\n",
        "        rmw,rml,trm = expl['RuleMatrix']; rows.append(dict(method='RuleMatrix', entropy_ratio=entropy_ratio(rmw), kl=kl_to_uniform(rmw), gini=gini_coeff(rmw), compactness=compactness_from_weights(rmw), speed=trm, identity=identity_score(rml), separability=separability_score(rml)))\n",
        "        single_df = pd.DataFrame(rows)\n",
        "        single_df.to_csv(f'./results_{name}.csv', index=False)\n",
        "        all_results[name] = single_df\n",
        "        Xe = pipe.named_steps['pre'].transform(X_train)\n",
        "        enc_stats[name] = dataset_distance(Xe, y_train.reset_index(drop=True))\n",
        "        gc.collect()\n",
        "    except Exception as e:\n",
        "        print('[skip]', name, ':', e)\n",
        "        continue\n",
        "\n",
        "summary_rows = []\n",
        "for name, df in all_results.items():\n",
        "    best_by_set = {}\n",
        "    for obj in METRIC_SETS:\n",
        "        tmp = df.copy()\n",
        "        if 'speed' in obj: tmp['speed'] = tmp['speed'].fillna(tmp['speed'].max())\n",
        "        pf = pareto_front(tmp, obj)\n",
        "        if 'speed' in pf.columns:\n",
        "            method = pf.sort_values('speed').iloc[0]['method']\n",
        "        else:\n",
        "            method = pf.iloc[0]['method']\n",
        "        best_by_set[str(obj)] = method\n",
        "    summary_rows.append({'dataset': name, 'acc': bb_scores.get(name, np.nan), **best_by_set})\n",
        "summary = pd.DataFrame(summary_rows)\n",
        "print('Best explainer per metric-set (direct evaluation):')\n",
        "display(summary)\n",
        "\n",
        "def nearest_dataset(target):\n",
        "    if target not in enc_stats: return None, None\n",
        "    d0 = 1e18; near=None\n",
        "    for other, st in enc_stats.items():\n",
        "        if other==target: continue\n",
        "        d = bures_distance_between(enc_stats[target], st)\n",
        "        if d < d0: d0=d; near=other\n",
        "    return near, d0\n",
        "\n",
        "meta_rows = []\n",
        "for name in all_results.keys():\n",
        "    neighbor, d = nearest_dataset(name)\n",
        "    recs = {}\n",
        "    if neighbor is not None:\n",
        "        df_nei = all_results[neighbor]\n",
        "        for obj in METRIC_SETS:\n",
        "            tmp = df_nei.copy()\n",
        "            if 'speed' in obj: tmp['speed'] = tmp['speed'].fillna(tmp['speed'].max())\n",
        "            pf = pareto_front(tmp, obj)\n",
        "            pick = pf.sort_values('speed').iloc[0]['method'] if 'speed' in pf.columns else pf.iloc[0]['method']\n",
        "            recs[str(obj)] = pick\n",
        "    meta_rows.append({'dataset': name, 'matched_dataset': neighbor, 'distance': d, **recs})\n",
        "meta = pd.DataFrame(meta_rows)\n",
        "print('Meta-recommendations via nearest dataset (Bures-like distance):')\n",
        "display(meta)\n",
        "\n",
        "summary.to_csv('./autoxai_results_summary.csv', index=False)\n",
        "meta.to_csv('./autoxai_meta_recommendations.csv', index=False)\n",
        "print('Saved: autoxai_results_summary.csv and autoxai_meta_recommendations.csv')\n"
      ]
