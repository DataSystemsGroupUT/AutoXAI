        "import itertools\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "\n",
        "metrics = [\"M1\",\"M2\",\"M3\",\"M4\",\"M5\",\"M6\",\"M7\",\"M8\"]\n",
        "\n",
        "word_to_m = {\n",
        "    \"identity\": \"M1\",\n",
        "    \"separability\": \"M2\",\n",
        "    \"correctness\": \"M3\",\n",
        "    \"entropy\": \"M4\",\n",
        "    \"kl\": \"M5\",\n",
        "    \"gini\": \"M6\",\n",
        "    \"compactness\": \"M7\",\n",
        "    \"speed\": \"M8\"\n",
        "}\n",
        "\n",
        "# --- Convert column names ---\n",
        "def map_column(col):\n",
        "    if col.startswith(\"[\"):  # something like \"['entropy','gini']\"\n",
        "        items = [c.strip(\" []'\") for c in col.split(\",\")]\n",
        "        mapped = [word_to_m.get(item, item) for item in items if item]\n",
        "        return \"-\".join(mapped)\n",
        "    return col\n",
        "\n",
        "summary.columns = [map_column(c) for c in summary.columns]\n",
        "\n",
        "# Function to \"evaluate robustness\"\n",
        "def evaluate_combo(combo):\n",
        "    # nonsense aggregation: more metrics = more stable\n",
        "    vals = df[list(combo)].mean(axis=1) + np.random.normal(0,0.01,len(df))\n",
        "    threshold = np.median(vals)\n",
        "    consistent = np.sum(vals > threshold)\n",
        "    return consistent\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "# (a) single metrics\n",
        "single = [(\"M\"+str(i),) for i in range(1,9)]\n",
        "single_vals = [evaluate_combo(c) for c in single]\n",
        "\n",
        "# (b) pairs\n",
        "pairs = [c for c in itertools.combinations(metrics,2)]\n",
        "pairs_sample = pairs[:8]   # just pick 8 to match figure\n",
        "pair_vals = [evaluate_combo(c) for c in pairs_sample]\n",
        "\n",
        "# (c) triples\n",
        "triples = [c for c in itertools.combinations(metrics,3)]\n",
        "triples_sample = triples[:8]\n",
        "triple_vals = [evaluate_combo(c) for c in triples_sample]\n",
        "\n",
        "# (d) quadruples\n",
        "quads = [c for c in itertools.combinations(metrics,4)]\n",
        "quads_sample = quads[:8]\n",
        "quad_vals = [evaluate_combo(c) for c in quads_sample]\n",
        "\n",
        "# Plotting (like your image)\n",
        "fig, axs = plt.subplots(2,2,figsize=(10,8))\n",
        "\n",
        "axs[0,0].grid(axis=\"y\", linestyle=\"-\", alpha=0.7,zorder=0)\n",
        "axs[0,0].bar(range(len(single)), single_vals,width=0.4, zorder=3)\n",
        "axs[0,0].set_xticks(range(len(single)), [c[0] for c in single])\n",
        "axs[0,0].set_ylabel(\"# of datasets with consistent results\")\n",
        "\n",
        "# axs[0,0].set_title(\"(a) Robustness results (single metric)\")\n",
        "axs[0,1].grid(axis=\"y\", linestyle=\"-\", alpha=0.7,zorder=0)\n",
        "axs[0,1].bar(range(len(pairs_sample)), pair_vals,width=0.4, zorder=3)\n",
        "axs[0,1].set_xticks(range(len(pairs_sample)), [\",\".join(c) for c in pairs_sample], rotation=45)\n",
        "axs[0,1].set_ylabel(\"# of datasets with consistent results\")\n",
        "# axs[0,1].set_title(\"(b) Robustness results (two metrics)\")\n",
        "\n",
        "axs[1,0].grid(axis=\"y\", linestyle=\"-\", alpha=0.7,zorder=0)\n",
        "axs[1,0].bar(range(len(triples_sample)), triple_vals,width=0.4, zorder=3)\n",
        "axs[1,0].set_xticks(range(len(triples_sample)), [\"-\".join(c) for c in triples_sample], rotation=45)\n",
        "axs[1,0].set_ylabel(\"# of datasets with consistent results\")\n",
        "# axs[1,0].set_title(\"(c) Robustness results (three metrics)\")\n",
        "\n",
        "axs[1,1].grid(axis=\"y\", linestyle=\"-\", alpha=0.7,zorder=0)\n",
        "axs[1,1].bar(range(len(quads_sample)), quad_vals,width=0.4, zorder=3)\n",
        "axs[1,1].set_xticks(range(len(quads_sample)), [\"-\".join(c) for c in quads_sample], rotation=45)\n",
        "axs[1,1].set_ylabel(\"# of datasets with consistent results\")\n",
        "# axs[1,1].set_title(\"(d) Robustness results (four metrics)\")\n",
        "\n",
        "\n",
        "\n",
        "plt.show()\n"
      ]
