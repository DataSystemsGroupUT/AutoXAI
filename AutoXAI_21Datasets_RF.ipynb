        "# 3) Utilities: data loading, pipeline, metrics, Pareto, dataset distance\n",
        "import numpy as np, pandas as pd, time\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.preprocessing import OneHotEncoder\n",
        "from sklearn.compose import ColumnTransformer\n",
        "from sklearn.pipeline import Pipeline\n",
        "from sklearn.metrics import accuracy_score\n",
        "from sklearn.ensemble import RandomForestClassifier\n",
        "import openml\n",
        "\n",
        "def load_dataset(spec):\n",
        "    try:\n",
        "        if isinstance(spec, int):\n",
        "            d = openml.datasets.get_dataset(spec)\n",
        "            X, y, _, _ = d.get_data(target=d.default_target_attribute, dataset_format='dataframe')\n",
        "        else:\n",
        "            from sklearn.datasets import fetch_openml\n",
        "            X, y = fetch_openml(spec, as_frame=True, return_X_y=True)\n",
        "        for c in X.select_dtypes(include=['object']).columns:\n",
        "            X[c] = X[c].astype('category')\n",
        "        return X, y\n",
        "    except Exception as e:\n",
        "        print('[skip] cannot load dataset', spec, ':', e)\n",
        "        return None, None\n",
        "\n",
        "def make_pipeline(X, use_autosklearn=False, rs=0):\n",
        "    cat_cols = X.select_dtypes(include=['category','object']).columns.tolist()\n",
        "    num_cols = [c for c in X.columns if c not in cat_cols]\n",
        "    pre = ColumnTransformer([('cat', OneHotEncoder(handle_unknown='ignore', sparse_output=False), cat_cols),\n",
        "                             ('num', 'passthrough', num_cols)])\n",
        "    if use_autosklearn:\n",
        "        import autosklearn.classification as ask\n",
        "        clf = ask.AutoSklearnClassifier(time_left_for_this_task=240, per_run_time_limit=40, seed=rs)\n",
        "    else:\n",
        "        clf = RandomForestClassifier(n_estimators=300, max_depth=None, random_state=rs, n_jobs=-1)\n",
        "    return Pipeline([('pre', pre), ('clf', clf)])\n",
        "\n",
        "def _norm(p): p=np.asarray(p,float); s=p.sum(); return p/s if s>0 else p\n",
        "def entropy_ratio(p): p=_norm(p); F=len(p); u=np.ones_like(p)/F; H=(p*np.log(p+1e-12)).sum(); Hu=(u*np.log(u+1e-12)).sum(); return H/Hu\n",
        "def kl_to_uniform(p): p=_norm(p); F=len(p); u=np.ones_like(p)/F; return (p*(np.log(p+1e-12)-np.log(u))).sum()\n",
        "def gini_coeff(p): p=_norm(p); return np.abs(p[:,None]-p[None,:]).sum()/(2*len(p)*(p.mean()+1e-12))\n",
        "def compactness_from_weights(p, thr=1e-9): p=np.asarray(p,float); return 1.0 - (np.abs(p)>thr).sum()/len(p)\n",
        "\n",
        "def identity_score(local_vectors, tol=1e-9):\n",
        "    ok=0; tot=0; groups={}\n",
        "    for k,v in local_vectors: groups.setdefault(k,[]).append(v)\n",
        "    for k,vs in groups.items():\n",
        "        if len(vs)>1:\n",
        "            tot += len(vs)-1\n",
        "            ref=vs[0]\n",
        "            for w in vs[1:]:\n",
        "                ok += 1 if np.allclose(ref,w,atol=tol) else 0\n",
        "    return ok/(tot if tot>0 else 1)\n",
        "\n",
        "def separability_score(local_vectors):\n",
        "    from numpy.linalg import norm\n",
        "    sims=[]; m=len(local_vectors)\n",
        "    for i in range(min(80,m)):\n",
        "        for j in range(i+1,min(80,m)):\n",
        "            vi=local_vectors[i][1]; vj=local_vectors[j][1]\n",
        "            sims.append(np.dot(vi,vj)/(norm(vi)*norm(vj)+1e-12))\n",
        "    return 1 - (np.mean(sims) if sims else 0.0)\n",
        "\n",
        "def pareto_front(df, objectives):\n",
        "    vals = df[objectives].values\n",
        "    keep = np.ones(len(df), dtype=bool)\n",
        "    for i in range(len(df)):\n",
        "        if not keep[i]: continue\n",
        "        for j in range(len(df)):\n",
        "            if i==j: continue\n",
        "            if np.all(vals[j] <= vals[i]) and np.any(vals[j] < vals[i]):\n",
        "                keep[i] = False; break\n",
        "    return df[keep]\n",
        "\n",
        "from scipy.linalg import sqrtm\n",
        "def _bures_gaussian(mu_a, cov_a, mu_b, cov_b):\n",
        "    dm = float(np.sum((mu_a-mu_b)**2))\n",
        "    ca12 = sqrtm(cov_a + 1e-9*np.eye(cov_a.shape[0]))\n",
        "    mid = ca12.dot(cov_b).dot(ca12)\n",
        "    inner = sqrtm(mid + 1e-9*np.eye(mid.shape[0]))\n",
        "    val = dm + float(np.trace(cov_a + cov_b - 2*inner).real)\n",
        "    return max(val, 0.0)\n",
        "\n",
        "def dataset_distance(X_enc, y):\n",
        "    classes = pd.unique(y)\n",
        "    stats = {}\n",
        "    for c in classes:\n",
        "        Z = X_enc[y==c]\n",
        "        if len(Z)<3: continue\n",
        "        mu = Z.mean(axis=0)\n",
        "        cov = np.cov(Z.T)\n",
        "        stats[str(c)] = (mu, cov)\n",
        "    return stats\n",
        "\n",
        "def bures_distance_between(stats_a, stats_b):\n",
        "    common = set(stats_a.keys()).intersection(set(stats_b.keys()))\n",
        "    if not common: return 1e12\n",
        "    d=0.0; k=0\n",
        "    for c in common:\n",
        "        mu_a, cov_a = stats_a[c]\n",
        "        mu_b, cov_b = stats_b[c]\n",
        "        d += _bures_gaussian(mu_a, cov_a, mu_b, cov_b)\n",
        "        k += 1\n",
        "    return d/max(k,1)\n"
      ]
    },
