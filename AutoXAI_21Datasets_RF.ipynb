        "# 4) Explainer pipelines\n",
        "from lime.lime_tabular import LimeTabularExplainer\n",
        "from anchor.anchor_tabular import AnchorTabularExplainer\n",
        "from imodels import RuleFitClassifier\n",
        "import rulematrix as rm\n",
        "from sklearn.preprocessing import LabelEncoder\n",
        "from sklearn.compose import ColumnTransformer\n",
        "from sklearn.preprocessing import OneHotEncoder, StandardScaler\n",
        "from sklearn.decomposition import PCA\n",
        "\n",
        "import warnings\n",
        "\n",
        "warnings.filterwarnings(\"ignore\")\n",
        "\n",
        "def run_explainers(pipe, X_train, X_test, max_lime=100, max_anchor=80, rf_depth=4, rs=0):\n",
        "    pre = pipe.named_steps['pre']\n",
        "\n",
        "    Xt = pre.transform(X_train)\n",
        "    Xe = pre.transform(X_test)\n",
        "    \n",
        "    feat_names = pre.get_feature_names_out().tolist()\n",
        "    class_labels = np.unique(pipe.predict(X_train)).astype(str).tolist()\n",
        "\n",
        "    lime = LimeTabularExplainer(Xt, feature_names=feat_names, class_names=class_labels, discretize_continuous=False)\n",
        "    rng = np.random.RandomState(rs)\n",
        "    idx = rng.choice(len(Xe), size=min(max_lime, len(Xe)), replace=False)\n",
        "    lime_w = pd.Series(0.0, index=feat_names); local_lime=[]\n",
        "    t0=time.time()\n",
        "    proba_fn = pipe.named_steps['clf'].predict_proba if hasattr(pipe.named_steps['clf'],'predict_proba') else None\n",
        "    for i in idx:\n",
        "\n",
        "        if proba_fn is None: break\n",
        "        exp = lime.explain_instance(Xe[i], proba_fn, num_features=10)\n",
        "        vec = np.zeros(len(feat_names))\n",
        "        for f,w in exp.as_list():\n",
        "            jj = [k for k,fname in enumerate(feat_names) if f==fname or f in fname]\n",
        "            if jj: vec[jj[0]] = w; lime_w[feat_names[jj[0]]] += abs(w)\n",
        "        local_lime.append((tuple(np.round(Xe[i],6)), vec))\n",
        "    t_lime = time.time()-t0\n",
        "    if lime_w.abs().sum()>0: lime_w /= lime_w.abs().sum()\n",
        "\n",
        "\n",
        "\n",
        "    anchor = AnchorTabularExplainer(class_names=class_labels, feature_names=feat_names, train_data=Xt)\n",
        "    counts = pd.Series(0.0, index=feat_names); local_anchor=[]\n",
        "    t0=time.time()\n",
        "    for i in rng.choice(len(Xe), size=min(max_anchor, len(Xe)), replace=False):\n",
        "        exp = anchor.explain_instance(Xe[i], pipe.named_steps['clf'].predict, threshold=0.95)\n",
        "        vec = np.zeros(len(feat_names))\n",
        "        for rule in exp.names():\n",
        "            for j,fn in enumerate(feat_names):\n",
        "                if fn in rule: counts[fn]+=1; vec[j]+=1\n",
        "        local_anchor.append((tuple(np.round(Xe[i],6)), vec))\n",
        "    t_anchor = time.time()-t0\n",
        "    anchor_w = counts / (counts.sum() + 1e-12)\n",
        "\n",
        "\n",
        "    rf = RuleFitClassifier(random_state=rs)\n",
        "    y_hat = pipe.predict(X_train)\n",
        "    le = LabelEncoder()\n",
        "    y_hat_num = le.fit_transform(y_hat)  \n",
        "\n",
        "    categorical_cols = X_train.select_dtypes(include=[\"object\", \"category\"]).columns\n",
        "    numeric_cols = X_train.select_dtypes(include=[\"number\"]).columns\n",
        "    \n",
        "    preprocessor = ColumnTransformer(\n",
        "        transformers=[\n",
        "            (\"cat\", OneHotEncoder(handle_unknown=\"ignore\"), categorical_cols),\n",
        "            (\"num\", StandardScaler(), numeric_cols)\n",
        "        ]\n",
        "    )\n",
        "    \n",
        "    X_train_preprocessed = preprocessor.fit_transform(X_train)\n",
        "\n",
        "\n",
        "\n",
        "    rf.fit(X_train_preprocessed, y_hat_num)\n",
        "\n",
        "    feature_importances_=rf._get_rules()\n",
        "\n",
        "\n",
        "\n",
        "    rf_w = pd.Series(feature_importances_['importance']).fillna(0)\n",
        "    if rf_w.abs().sum()>0: rf_w /= rf_w.abs().sum()\n",
        "    local_rf = [(None, rf_w.values.copy()) for _ in range(min(60, len(X_test)))]\n",
        "    X_test_aligned = X_test[X_train.columns]\n",
        "\n",
        "    X_test_preprocessed = preprocessor.transform(X_test_aligned)\n",
        "    \n",
        "    # --- Step 9: Compute fidelity ---\n",
        "    rf_fidelity = (pipe.predict(X_test_aligned) == rf.predict(X_test_preprocessed)).mean()\n",
        "\n",
        "\n",
        "\n",
        "    Xt_reduced = PCA(n_components=min(X_train_preprocessed.shape[0], X_train_preprocessed.shape[1])-1).fit_transform(X_train_preprocessed)\n",
        "    class_mapping = {'bad': 0, 'good': 1}\n",
        "\n",
        "    def teacher_predict(Xraw):\n",
        "\n",
        "\n",
        "    \n",
        "        preds = pipe.named_steps['clf'].predict(Xraw)\n",
        "        return np.array([class_mapping[p] for p in preds])\n",
        "        # return pipe.named_steps['clf'].predict(Xt)\n",
        "    \n",
        "    sur = rm.Surrogate(teacher=teacher_predict)\n",
        "    t0=time.time()\n",
        "\n",
        "\n",
        "    Xt_reduced = np.hstack([Xt_reduced, np.random.rand(Xt_reduced.shape[0], 1)])\n",
        "    sur.fit(Xt_reduced)\n",
        "    t_rm = time.time()-t0\n",
        "    rules = str(sur.teacher)\n",
        "\n",
        "\n",
        "    rm_counts = pd.Series([rules.count(f) for f in X_train.columns], index=X_train.columns)\n",
        "    rm_w = rm_counts / (rm_counts.sum() + 1e-12)\n",
        "    local_rm = [(None, rm_w.values.copy()) for _ in range(min(60, len(X_test)))]\n",
        "\n",
        "    return {\n",
        "        'LIME': (lime_w, local_lime, t_lime),\n",
        "        'ANCHOR': (anchor_w, local_anchor, t_anchor),\n",
        "        'RuleFit': (rf_w, local_rf, rf_fidelity),\n",
        "        'RuleMatrix': (rm_w, local_rm, t_rm)\n",
        "    }\n"
      ]
    },
